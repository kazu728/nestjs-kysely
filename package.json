{
  "name": "@kzmat/nestjs-kysely",
  "version": "0.0.1",
  "description": "Kysely module for NestJS",
  "keywords": [
    "kysely",
    "nestjs"
  ],
  "author": "kzmat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kzmat/nestjs-kysely"
  },
  "bugs": "https://github.com/kzmat/nestjs-kysely/issues",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf ./dist",
    "fix:eslint": "eslint --fix --ext ts,tsx ./src",
    "fix:prettier": "prettier --write \"./**/*.{md,json}\"",
    "link:package": "yarn && yarn build && cp -r dist ./node_modules && mv ./node_modules/dist ./node_modules/nestjs-kysely",
    "lint:eslint": "eslint --ext ts,tsx ./src",
    "lint:prettier": "prettier --check \"./**/*.{md,json}\"",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.2",
    "@nestjs/core": "^8.2.2",
    "kysely": "^0.8.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.3",
    "@types/eslint": "^8.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.0.3",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
